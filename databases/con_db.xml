This file holds values related to constructions for ESP-r-->

<constructions>

<!--	construction (define a construction for each construction type)
		name =  construction name
		type = OPAQ (opaque) or TRAN (translucent)
		symmetry = SYMMETRIC or NONSYMMETRIC
		description =


		IF (type = "TRAN") THEN {
		optic_name = TMC optic name
		}

		layers (one for each material layers, from outside to inside)
			mat = material name (see mat_db_xml_list)
			thickness_mm = layers thickness in meters

			IF (mat = "Gap") {
			gap_RSI
				vert = RSI for vertical placement
				horiz = horizontal
				slope = sloped
			}-->


<!-- BASEMENT -->
	<construction name="B_slab" type="OPAQ" symmetry="SYMMETRIC" description="Basement concrete slab without insulation">
		<layers mat="Concrete" thickness_mm="76" component="slab"/>
	</construction>
	
	<construction name="B_slab_bot" type="OPAQ" symmetry="NONSYMMETRIC" description="Basement concrete slab with bottom insulation">
		<layers mat="EPS" thickness_mm="38" component="insulation_2"/>
		<layers mat="Concrete" thickness_mm="76" component="slab"/>
	</construction>

	<construction name="B_slab_top" type="OPAQ" symmetry="NONSYMMETRIC" description="Basement concrete slab with top insulation">
		<layers mat="Concrete" thickness_mm="76" component="slab"/>
		<layers mat="EPS" thickness_mm="38" component="insulation_1"/>
	</construction>
	
	<construction name="B_wall" type="OPAQ" symmetry="SYMMETRIC" description="Basement concrete wall without insulation">
		<layers mat="Concrete" thickness_mm="203" component="wall"/>
	</construction>

	<construction name="B_wall_adb" type="OPAQ" symmetry="SYMMETRIC" description="Basement shared adiabatic concrete wall (only 102 mm of 203 mm thickness)">
		<layers mat="Concrete" thickness_mm="102" component="wall"/>
	</construction>

	<construction name="B_wall_pony" type="OPAQ" symmetry="NONSYMMETRIC" description="Basement multiplayer pony wall - similar to main wall">
		<layers mat="Vinyl" thickness_mm="3" component="siding"/>
		<layers mat="OSB" thickness_mm="15" component="sheathing"/>
		<layers mat="Fbrglas_Batt" thickness_mm="140" component="insulation_1"/>
		<layers mat="Drywall" thickness_mm="12" component="interior"/>
	</construction>

	<construction name="B->M" type="OPAQ" symmetry="SYMMETRIC" description="reverse of MAIN_BSMT; 40 mm thick plywood separating main from basement">
		<layers mat="Plywood" thickness_mm="40" component="sheathing"/>
	</construction>


